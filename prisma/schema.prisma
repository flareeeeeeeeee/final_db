generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["relationJoins"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model contrata_config {
  id          Int     @id @default(autoincrement())
  name        String? @db.VarChar(255)
  am_capacity Int?
  pm_capacity Int?
}

model ref_cita {
  id           Int       @id @default(autoincrement())
  id_contrata  Int?
  id_user      Int?      @default(0)
  id_client    Int?      @default(0)
  id_case      String?   @default("0") @db.VarChar(255)
  address      String?   @db.VarChar(255)
  date         String?   @db.VarChar(255)
  am_pm        String?   @db.VarChar(2)
  created_at   DateTime? @default(now()) @db.DateTime(0)
  cod_cuenta   String?   @db.VarChar(255)
  id_agente    Int?
  numero_orden String?   @db.VarChar(255)
  status_id    Int       @default(1)

  lat              String? @db.VarChar(255)
  long             String? @db.VarChar(255)
  contact1         String? @db.VarChar(255)
  contact2         String? @db.VarChar(255)
  nombre_entrega   String? @db.VarChar(255)
  contacto_entrega String? @db.VarChar(255)

  status ref_cita_status @relation(fields: [status_id], references: [id])

  @@index([id_contrata], map: "id_contrata")
}

model ref_cita_status {
  id       Int        @id @default(autoincrement())
  name     String     @db.VarChar(255)
  ref_cita ref_cita[]
}

model ref_cita_status_hist {
  id         Int       @id @default(autoincrement())
  id_cita    Int
  new_status Int
  old_status Int
  lat        String?   @db.VarChar(255)
  long       String?   @db.VarChar(255)
  updated_at DateTime? @default(now()) @db.DateTime(0)
  updated_by Int
}

model ref_config {
  name  String  @id @db.VarChar(255)
  value String? @db.VarChar(255)
}

model ref_cita_equipos {
  id           Int       @id @default(autoincrement())
